{
	"schemaVersion": 1,
	"id": "scriptedmodels",
	"version": "${version}",
	"name": "Scripted Models",
	  "description": "a Character Editor using Scripts",
	  "authors": [
		"PixelDots"
	  ],
	  "contact": {
		"homepage": "https://modrinth.com/mod/scripted-models",
		"sources": "https://github.com/PixelDoted/Scripted-Models"
	  },
